{
	"name": "kufar-bot",
	"dockerComposeFile": [
		"../docker-compose.yml",
		"docker-compose.devcontainer.yml"
	],
	"service": "kufar-bot",
	"workspaceFolder": "/app",
	"shutdownAction": "stopCompose",
	"remoteUser": "root",
	"customizations": {
		"vscode": {
			"extensions": [
				"biomejs.biome",
				"mhutchie.git-graph",
				"streetsidesoftware.code-spell-checker",
				"streetsidesoftware.code-spell-checker-russian"
			]
		}
	}
}
